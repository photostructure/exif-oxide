{
  "$schema": "../schema.json",
  "description": "Undef keyword returning empty string (migrated from numeric_string_ops.rs)",
  "expression": "undef",
  "type": "PrintConv",
  "exiftool_reference": "undefined values return empty string in PrintConv",
  "test_cases": [
    {
      "description": "Undef returns empty string",
      "input": {
        "U32": 42
      },
      "expected": {
        "String": ""
      }
    },
    {
      "description": "Undef with string input",
      "input": {
        "String": "test"
      },
      "expected": {
        "String": ""
      }
    }
  ]
}

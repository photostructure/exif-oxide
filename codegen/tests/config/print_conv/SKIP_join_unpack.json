{
  "$schema": "../schema.json",
  "description": "Join and unpack pattern for hex formatting",
  "expression": "join \" \", unpack \"H2H2\", $val",
  "type": "PrintConv",
  "exiftool_reference": "lib/Image/ExifTool/Panasonic.pm:234",
  "test_cases": [
    {
      "description": "Two-byte binary to hex string",
      "input": {
        "Binary": [
          171,
          205
        ]
      },
      "expected": {
        "String": "ab cd"
      }
    },
    {
      "description": "Empty binary data",
      "input": {
        "Binary": []
      },
      "expected": {
        "String": ""
      }
    }
  ]
}

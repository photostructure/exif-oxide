{
  "$schema": "../schema.json",
  "description": "Length function without parentheses",
  "expression": "length $val",
  "type": "PrintConv",
  "exiftool_reference": "Perl's length function can be called without parens",
  "test_cases": [
    {
      "description": "Get length of string",
      "input": {
        "String": "Hello"
      },
      "expected": {
        "String": "5"
      }
    },
    {
      "description": "Empty string",
      "input": {
        "String": ""
      },
      "expected": {
        "String": "0"
      }
    }
  ]
}

{
  "$schema": "../../schemas/simple_table.json",
  "source": "third-party/exiftool/lib/Image/ExifTool/Nikon.pm",
  "description": "Nikon module lookup tables for lenses, camera settings, and modes",
  "tables": [
    {
      "hash_name": "%nikonLensIDs",
      "constant_name": "NIKON_LENS_IDS",
      "key_type": "String",
      "description": "Nikon lens identification database"
    },
    {
      "hash_name": "%afPoints153",
      "constant_name": "AF_POINTS_153",
      "key_type": "u8",
      "description": "AF point indices for 153-point AF system"
    },
    {
      "hash_name": "%afPoints135",
      "constant_name": "AF_POINTS_135",
      "key_type": "u8",
      "description": "AF point indices for 135-point AF system"
    },
    {
      "hash_name": "%afPoints105",
      "constant_name": "AF_POINTS_105",
      "key_type": "u8",
      "description": "AF point indices for 105-point AF system"
    },
    {
      "hash_name": "%iSOAutoShutterTimeZ9",
      "constant_name": "ISO_AUTO_SHUTTER_TIME_Z9",
      "key_type": "String",
      "description": "Z9 ISO auto shutter time mappings"
    },
    {
      "hash_name": "%focusModeZ7",
      "constant_name": "FOCUS_MODE_Z7",
      "key_type": "u8",
      "description": "Nikon Z7/Z8/Z9 focus mode settings"
    },
    {
      "hash_name": "%nefCompression",
      "constant_name": "NEF_COMPRESSION",
      "key_type": "u8",
      "description": "Nikon NEF (RAW) compression modes"
    },
    {
      "hash_name": "%meteringModeZ7",
      "constant_name": "METERING_MODE_Z7",
      "key_type": "u8",
      "description": "Nikon Z7/Z8/Z9 metering mode settings"
    }
  ]
}

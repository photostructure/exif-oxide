    {
      "hash_name": "%xmpNS",
      "constant_name": "XMP_GROUP_NAMES",
      "key_type": "String",
      "description": "ExifTool XMP family 1 group name translations"
    },
    {
      "hash_name": "%charName",
      "constant_name": "XML_CHAR_NAMES",
      "key_type": "String",
      "description": "XML character to entity name mappings"
    },
    {
      "hash_name": "%charNum",
      "constant_name": "XML_CHAR_NUMS",
      "key_type": "String",
      "value_type": "u32",
      "description": "XML entity name to character code mappings"
    },
    {
      "hash_name": "%stdXlatNS",
      "constant_name": "STD_XLAT_NS",
      "key_type": "String",
      "description": "Standard XMP namespace prefix translations"
    }
  ]

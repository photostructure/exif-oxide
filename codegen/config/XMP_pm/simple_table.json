{
  "$schema": "../../schemas/simple_table.json",
  "source": "third-party/exiftool/lib/Image/ExifTool/XMP.pm",
  "description": "XMP module lookup tables for namespace mappings and character conversions",
  "tables": [
    {
      "hash_name": "%nsURI",
      "constant_name": "NAMESPACE_URIS",
      "key_type": "String",
      "description": "XMP namespace prefix to URI mappings"

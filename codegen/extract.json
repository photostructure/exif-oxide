{
  "$schema": "./extract_schema.json",
  "description": "Configuration for ExifTool data extraction",
  "tables": [
    {
      "module": "Nikon.pm",
      "hash_name": "%nikonLensIDs",
      "output_file": "nikon/lenses.rs",
      "constant_name": "NIKON_LENS_IDS",
      "key_type": "String",
      "description": "Nikon lens identification database"
    },
    {
      "module": "Canon.pm",
      "hash_name": "%canonModelID",
      "output_file": "canon/models.rs",
      "constant_name": "CANON_MODEL_ID",
      "key_type": "u32",
      "description": "Canon camera model identification"
    },
    {
      "module": "Canon.pm",
      "hash_name": "%canonWhiteBalance",
      "output_file": "canon/white_balance.rs",
      "constant_name": "CANON_WHITE_BALANCE",
      "key_type": "u8",
      "description": "White balance mode names"
    },
    {
      "module": "Canon.pm",
      "hash_name": "%pictureStyles",
      "output_file": "canon/picture_styles.rs",
      "constant_name": "PICTURE_STYLES",
      "key_type": "u16",
      "description": "Picture style mode names"
    },
    {
      "module": "Canon.pm",
      "hash_name": "%canonImageSize",
      "output_file": "canon/image_size.rs",
      "constant_name": "CANON_IMAGE_SIZE",
      "key_type": "i16",
      "description": "Image size setting names"
    },
    {
      "module": "Canon.pm",
      "hash_name": "%canonQuality",
      "output_file": "canon/quality.rs",
      "constant_name": "CANON_QUALITY",
      "key_type": "i16",
      "description": "Image quality setting names"
    },
    {
      "module": "ExifTool.pm",
      "hash_name": "%magicNumber",
      "output_file": "file_types/magic_numbers.rs",
      "constant_name": "MAGIC_NUMBER_PATTERNS",
      "key_type": "String",
      "extraction_type": "regex_strings",
      "description": "Magic number regex patterns for file type detection"
    },
    {
      "module": "ExifTool.pm",
      "hash_name": "%fileTypeLookup",
      "output_file": "file_types/file_type_lookup.rs",
      "constant_name": "FILE_TYPE_LOOKUP",
      "key_type": "String",
      "extraction_type": "file_type_lookup",
      "description": "Core file type detection lookup table"
    },
    {
      "module": "ExifTool.pm",
      "hash_name": "%mimeType",
      "output_file": "file_types/mime_types.rs",
      "constant_name": "MIME_TYPES",
      "key_type": "String",
      "description": "MIME type mappings for file types"
    },
    {
      "module": "ExifTool.pm",
      "hash_name": "%fileTypeExt",
      "output_file": "file_types/file_extensions.rs",
      "constant_name": "FILE_TYPE_EXTENSIONS",
      "key_type": "String",
      "description": "Canonical file extensions for file types"
    },
    {
      "module": "ExifTool.pm",
      "hash_name": "%weakMagic",
      "output_file": "file_types/weak_magic_types.rs",
      "constant_name": "WEAK_MAGIC_TYPES",
      "key_type": "String",
      "extraction_type": "boolean_set",
      "description": "File types with weak magic numbers that defer to extension"
    },
    {
      "module": "Exif.pm",
      "hash_name": "%orientation",
      "output_file": "exif/orientation.rs",
      "constant_name": "ORIENTATION",
      "key_type": "u8",
      "description": "EXIF Orientation tag PrintConv values"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%nsURI",
      "output_file": "xmp/namespace_uris.rs",
      "constant_name": "NAMESPACE_URIS",
      "key_type": "String",
      "description": "XMP namespace prefix to URI mappings"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%xmpNS",
      "output_file": "xmp/group_names.rs",
      "constant_name": "XMP_GROUP_NAMES",
      "key_type": "String",
      "description": "ExifTool XMP family 1 group name translations"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%charName",
      "output_file": "xmp/char_names.rs",
      "constant_name": "XML_CHAR_NAMES",
      "key_type": "String",
      "description": "XML character to entity name mappings"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%charNum",
      "output_file": "xmp/char_nums.rs",
      "constant_name": "XML_CHAR_NUMS",
      "key_type": "String",
      "value_type": "u32",
      "description": "XML entity name to character code mappings"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%stdXlatNS",
      "output_file": "xmp/std_xlat_ns.rs",
      "constant_name": "STD_XLAT_NS",
      "key_type": "String",
      "description": "Standard XMP namespace prefix translations"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%latConv",
      "output_file": "xmp/lat_conv.rs",
      "constant_name": "LAT_CONV",
      "key_type": "String",
      "description": "GPS latitude hemisphere conversion"
    },
    {
      "module": "XMP.pm",
      "hash_name": "%longConv",
      "output_file": "xmp/long_conv.rs",
      "constant_name": "LONG_CONV",
      "key_type": "String",
      "description": "GPS longitude hemisphere conversion"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%isoAutoHiLimitZ7",
      "output_file": "nikon/iso_auto_hi_limit.rs",
      "constant_name": "ISO_AUTO_HI_LIMIT_Z7",
      "key_type": "u8",
      "extraction_type": "print_conv",
      "description": "ISO Auto Hi Limit settings for Z7 series"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%focusModeZ7",
      "output_file": "nikon/focus_mode.rs",
      "constant_name": "FOCUS_MODE_Z7",
      "key_type": "u8",
      "description": "Focus mode settings for Z7 series"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%aFAreaModePD",
      "output_file": "nikon/af_area_mode.rs",
      "constant_name": "AF_AREA_MODE_PD",
      "key_type": "u8",
      "description": "AF area mode phase detect settings"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%activeDLightingZ7",
      "output_file": "nikon/active_d_lighting.rs",
      "constant_name": "ACTIVE_D_LIGHTING_Z7",
      "key_type": "u8",
      "description": "Active D-Lighting strength levels"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%hdrLevelZ8",
      "output_file": "nikon/hdr_level.rs",
      "constant_name": "HDR_LEVEL_Z8",
      "key_type": "u8",
      "description": "HDR level settings for Z8 series"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%subjectDetectionZ9",
      "output_file": "nikon/subject_detection.rs",
      "constant_name": "SUBJECT_DETECTION_Z9",
      "key_type": "u8",
      "description": "Subject detection modes for Z9 series"
    },
    {
      "module": "Nikon.pm",
      "hash_name": "%nefCompression",
      "output_file": "nikon/nef_compression.rs",
      "constant_name": "NEF_COMPRESSION",
      "key_type": "u8",
      "description": "NEF (RAW) compression modes"
    }
  ]
}
